{
  "name": "toretto-css",
  "version": "0.0.5",
  "description": "Toretto CSS Framework",
  "main": "index.js",
  "scripts": {
    "copy": "rm -rf ./public/v/current && cp -r ./public/v/dev ./public/v/current && cp -r ./public/v/dev ./public/v/${npm_package_version} && echo '${npm_package_version}' > ./public/v/${npm_package_version}/version.txt",
    "release": "standard-version && npm run copy",
    "dev": "live-server ./public --ignore=./node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "CSS",
    "JS",
    "CSS in JS"
  ],
  "author": "Rus INozemtsev",
  "license": "ISC",
  "devDependencies": {
    "live-server": "^1.2.1",
    "standard-version": "^9.0.0"
  }
}
